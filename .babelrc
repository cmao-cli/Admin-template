{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-react",
  ],
  "plugins": [
    ["import", {
      "libraryName": "antd",
      "style": "css"
    }],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/plugin-syntax-dynamic-import",
    [
      "@babel/plugin-syntax-decorators",  // 支持装饰器
      {
        "legacy": true,
      }
    ],
    ["react-hot-loader/babel"]
  ]
}